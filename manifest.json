{
  "manifest_version": 2,

  "name": "Focus Popup",
  "description": "Open the page in a popup window",
  "version": "1.8",
  "icons": {
	  "16": "icon16.png",
	  "38": "icon38.png",
	  "48": "icon48.png",
	  "64": "icon64.png"
  },
  
  "omnibox": {
  	"keyword" : "focus"
  },

  "browser_action": {
    "default_icon": "icon.png"
  },
  
  "background": {
    "scripts": ["background.js"]
  },
  
  "permissions": [
    "contextMenus", "activeTab", "*://*/*", "tabs"
  ]
}
